{
  "rules": {
    "body": [
      {
        "when": {
          "bodySeries": "BMT"
        },
        "hideOptions": [
          {
            "field": "bodyLength",
            "values": [
              "14'",
              "14' 6\"",
              "15'",
              "15' 6\"",
              "16'",
              "16' 6\"",
              "20'"
            ]
          }
        ]
      },
      {
        "when": {
          "bodySeries": "BFL"
        },
        "hideOptions": [
          {
            "field": "bodyLength",
            "values": [
              "14' 6\"",
              "15' 6\"",
              "16' 6\""
            ]
          }
        ]
      },
      {
        "when": {
          "doorSlope": "12\""
        },
        "hideOptions": [
          {
            "field": "hiLiftTailgate",
            "values": ["Yes"]
          }
        ]
      },

      {
        "when": {
          "sideHtR": "66R"
        },
        "hideOptions": [
          {
            "field": "sideHtF",
            "values": ["60F", "54F", "48F"]
          }
        ]
      },
      {
        "when": {
          "sideHtR": "60R"
        },
        "hideOptions": [
          {
            "field": "sideHtF",
            "values": ["54F", "48F"]
          }
        ]
      },
      {
        "when": {
          "sideHtR": "54R"
        },
        "hideOptions": [
          {
            "field": "sideHtF",
            "values": ["48F"]
          }
        ]
      },
      {
        "when": {
          "sideHtR": "48R"
        },
        "hideOptions": [
          {
            "field": "sideHtF",
            "values": []
          }
        ]
      },

      {
        "when": {
          "sideHtF": "66F"
        },
        "hideOptions": [
          {
            "field": "sideHtR",
            "values": ["48R"]
          }
        ]
      },
      {
        "when": {
          "sideHtF": "60F"
        },
        "hideOptions": [
          {
            "field": "sideHtR",
            "values": ["66R"]
          }
        ]
      },
      {
        "when": {
          "sideHtF": "54F"
        },
        "hideOptions": [
          {
            "field": "sideHtR",
            "values": ["66R", "60R"]
          }
        ]
      },
      {
        "when": {
          "sideHtF": "48F"
        },
        "hideOptions": [
          {
            "field": "sideHtR",
            "values": ["66R", "60R", "54R"]
          }
        ]
      },

      {
        "when": {
          "sideHtF": "66F",
          "sideHtR": "54R"
        },
        "addItems": [
          {
            "key": "side_ht_adder_66F_54R",
            "itemNumber": "SRG80-66F54R",
            "hidden": true,
            "specLabel": "Side Heights",
            "specValue": "66F / 54R"
          }
        ]
      },
      {
        "when": {
          "sideHtF": "60F",
          "sideHtR": "48R"
        },
        "addItems": [
          {
            "key": "side_ht_adder_60F_48R",
            "itemNumber": "SRG80-60F48R",
            "hidden": true,
            "specLabel": "Side Heights",
            "specValue": "60F / 48R"
          }
        ]
      }
    ],
    "truck": [
      {
        "when": {
          "hitchPlate": "Pintle Plate Only"
